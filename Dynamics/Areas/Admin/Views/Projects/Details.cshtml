        <div class="content-page">
            <div class="content">

                <!-- Start Content-->
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box">
                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item active">Admin</li>
                                        <li class="breadcrumb-item"><a href="/Admin/Projects">Projects</a></li>
                                        <li class="breadcrumb-item"><a href="#">Details</a></li>
                                    </ol>
                                </div>
                                <h4 class="page-title">Project Details</h4>
                            </div>
                        </div>
                    </div>
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-xxl-12 col-lg-12">
                            <!-- project card -->
                            <div class="card d-block">
                                <div class="card-body">
                                    <div class="d-flex justify-content-between align-items-center mb-2">
                                        <h3 class="mt-0">@Html.DisplayFor(model => model.ProjectName)</h3>
                                    </div>

                                    <div class="badge @GetStatusClass(Model.ProjectStatus)" data-project-id="@Model.ProjectID" data-project-status="@Model.ProjectStatus">
                                        @GetStatusText(Model.ProjectStatus)
                                    </div>
                                    <h5>Description:</h5>

                                    <p class="text-muted mb-2">
                                        @Html.DisplayFor(model => model.ProjectDescription)
                                    </p>

                                    <br/>

                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Address</h5>
                                                <p>@Html.DisplayFor(model => model.ProjectAddress)</p>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Email</h5>
                                                <p>@Html.DisplayFor(model => model.ProjectEmail)</p>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Phone number</h5>
                                                <p>@Html.DisplayFor(model => model.ProjectPhoneNumber)</p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Start Date</h5>
                                                <p>@Html.DisplayFor(model => model.StartTime)</p>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>End Date</h5>
                                                <p class="end-date" data-project-id="@Model.ProjectID">
                                                    @if (Model.isBanned)
                                                    {
                                                        <span class="text-danger">Banned</span>
                                                    }
                                                    else if (Model.isBanned == false && Model.ProjectStatus == -1)
                                                    {
                                                        <span class="text-danger">Canceled</span>
                                                    }
                                                    else if (Model.isBanned == false && Model.ProjectStatus == 0)
                                                    {
													}
                                                    else if (Model.isBanned == false && Model.ProjectStatus == 1)
													{
														
													}
                                                    else
                                                    {
                                                        if (Model.EndTime == null)
														{
															<span class="text-danger">Not set</span>
														}
														else
														{
															@Html.DisplayFor(model => model.EndTime)
														}
                                                    }
                                                </p>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Shutdown Reason</h5>
                                                @if (Model.ShutdownReason == null)
												{
													<p>None</p>
												}
												else
												{
													<p>@Html.DisplayFor(model => model.ShutdownReason)</p>
												}
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div id="tooltip-container">
                                                <h5>Team Members</h5>
                                                @foreach (var members in Model.ProjectMember)
                                                {
                                                    <a href="/user/@members.User.UserName" data-bs-container="#tooltip-container" data-bs-toggle="tooltip" data-bs-placement="top" title="@members.User.UserName" class="d-inline-block">
                                                        <img src="@members.User.UserAvatar" class="rounded-circle img-thumbnail avatar-sm" alt="friend">
                                                    </a>
                                                }
                                            </div>
                                        </div>
                                            
                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Thumbnail</h5>
                                                <div id="carouselExample" class="carousel slide" data-bs-ride="carousel">
                                                    <div class="carousel-inner">
                                                        @if (Model.Attachment != null)
                                                        {
                                                            var images = Model.Attachment.Split(",");
                                                            bool isActive = true;

                                                            foreach (var image in images)
                                                            {
                                                            <div class="carousel-item @(isActive ? "active" : "")">
                                                                <img src="@image.Trim()" class="d-block w-100" alt="Project Image">
                                                            </div>
                                                            isActive = false;
                                                            }
                                                        }
                                                    </div>
                                                    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
                                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                                        <span class="visually-hidden">Previous</span>
                                                    </button>
                                                    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
                                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                                        <span class="visually-hidden">Next</span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="col-md-4">
                                            <div class="mb-4">
                                                <h5>Status</h5>
                                                @if (Model.ProjectStatus == -1 && !Model.isBanned)
                                                {
                                                    <span class="badge badge-danger-lighten">Canceled</span>
                                                }
                                                else if (Model.ProjectStatus == 0 && !Model.isBanned)
                                                {
                                                    <span class="badge badge-warning-lighten">Preparing</span>
                                                }
                                                else if (Model.ProjectStatus != 2)
                                                {
                                                    <a href="#" class="btn @(Model.isBanned ? "badge badge-danger-lighten" : "badge badge-success-lighten") btn-ban" data-id="@Model.ProjectID">
                                                        @(Model.isBanned ? "Banned" : "Active")
                                                    </a>
                                                }
                                                else
                                                {
                                                    <span class="badge badge-success-lighten">Finished</span>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div> <!-- end card-body-->
                            </div> <!-- end card-->
                        </div> <!-- end col -->
                    </div>
                    <!-- end row -->
                        
                </div> <!-- container -->

            </div> <!-- content -->
            <!-- Footer Start -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6">
