                                <a href="https://facebook.com/kleqing">Support</a>
                                <a href="mailto:hotaruu24k@gmail.com">Contact Us</a>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
            <!-- end Footer -->

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->

