<!DOCTYPE html>
<html lang="en" data-theme="light">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Dynamics</title>
    @* Custom css - tailwind - bootstrap *@
    <link href="css/original.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
</head>
<body class="bg-base-100 relative">

    @RenderBody()
    @* This is used for icons *@
    <script src="https://kit.fontawesome.com/ba8c3661b4.js" crossorigin="anonymous"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <partial name="CustomComponent/_LoadingBackdrop"/>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>